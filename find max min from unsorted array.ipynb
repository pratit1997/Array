    "def find_Largest(x):\n",
    "    n=len(x)\n",
    "    largest=x[0]\n",
    "    smallest=x[0]\n",
    "    for i in x:\n",
    "        if i>largest:\n",
    "            largest=i\n",
    "        elif i<smallest:\n",
    "            smallest=i\n",
    "    return largest,smallest\n",
    "\n",
    "\n",
    "array=[6,2,7,9,4,6,3,6,3]\n",
    "output=find_Largest(array)\n",
    "print(output)"
